{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/generateimage", "dest": "app.js" },
    { "src": "/generateimageA", "dest": "app.js" },
    { "src": "/favicon.ico", "dest": "favicon.ico" },
    { "src": "(.*)", "dest": "app.js" }
  ]
}
